<p id="notice"><%= notice %></p>

<h1>Usuários</h1>

<table border="1">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>CPF</th>
            <th>Data de nascimento</th>
            <th>Telefone</th>
            <th>Gênero</th>
        </tr>
    </thead>

    <tbody>
        <% @users.each do |users| %>
            <tr>
                <td><%= users.name %></td>
                <td><%= users.email %></td>
                <td><%= users.cpf %></td>
                <td><%= users.birth_date %></td>
                <td><%= users.phone %></td>
                <td><%= users.gender %></td>
                <td><%= link_to 'Show', users %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'Novo usuário', new_user_path %>
<br>
<%= link_to 'Voltar', root_path %>